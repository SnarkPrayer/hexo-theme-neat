<article class="archive-article archive-type-<%= post.layout %>">
  <div class="archive-article-inner">
    <%- partial('post/date', {post: post, class_name: 'archive-article-date', date_format: 'MMM D'}) %>
    <%- partial('post/title', {post: post, index: true, class_name: 'archive-article-title'}) %>
  </div>
</article>
